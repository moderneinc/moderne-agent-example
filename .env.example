# ============================================================================
# CORE AGENT CONFIGURATION (Required)
# ============================================================================

# URI for connecting to the Moderne API Gateway (provided by Moderne)
# Format: https://api.<tenant>.moderne.io
MODERNE_AGENT_APIGATEWAYRSOCKETURI=https://api.<tenant>.moderne.io

# 256-bit AES encryption key in hex format for artifact encryption
# Generate with: openssl enc -aes-256-cbc -k secret -P -md sha256
# Use the value after "key="
MODERNE_AGENT_CRYPTO_SYMMETRICKEY=<your-256-bit-hex-key>

# Identifier for this agent in the Moderne dashboard
# This will be visible in metrics and the UI (use lowercase with hyphens)
MODERNE_AGENT_NICKNAME=my-agent

# Agent connection token (provided by Moderne)
MODERNE_AGENT_TOKEN=<your-agent-token>

# ============================================================================
# PERFORMANCE TUNING (Optional)
# ============================================================================

# Number of parallel threads for downloading LSTs (default: 2)
# Increase for better throughput if network and CPU allow
#MODERNE_AGENT_DOWNLOADPARALLELISM=4

# Frequency of LST artifact index updates in seconds (default: 120)
# Lower values provide fresher data but increase load
#MODERNE_AGENT_ARTIFACTINDEXINTERVALSECONDS=120

# ============================================================================
# REPOSITORY CONFIGURATION (Optional)
# ============================================================================

# Load repository list from HTTP(S) endpoint (repos.csv format)
#MODERNE_AGENT_ORGANIZATION_REPOSCSV=https://example.com/repos.csv

# ============================================================================
# COMMIT OPTIONS (Optional)
# ============================================================================

# Restrict available commit strategies
# Options: Direct, Branch, Fork, PullRequest, ForkAndPullRequest
#MODERNE_AGENT_DEFAULTCOMMITOPTIONS_0=PullRequest
#MODERNE_AGENT_DEFAULTCOMMITOPTIONS_1=ForkAndPullRequest

# ============================================================================
# GITHUB CONFIGURATION (Optional)
# ============================================================================

# GitHub OAuth credentials for authentication
#MODERNE_AGENT_GITHUB_0_OAUTH_CLIENTID=<github-oauth-client-id>
#MODERNE_AGENT_GITHUB_0_OAUTH_CLIENTSECRET=<github-oauth-client-secret>

# GitHub instance URL (use https://github.com for GitHub.com)
#MODERNE_AGENT_GITHUB_0_URL=https://github.com

# Organizations allowed to use this agent (supports wildcards)
#MODERNE_AGENT_GITHUB_0_ALLOWABLE_ORGANIZATIONS_0=my-org
#MODERNE_AGENT_GITHUB_0_ALLOWABLE_ORGANIZATIONS_1=another-org

# Skip SSL verification (not recommended for production)
#MODERNE_AGENT_GITHUB_0_SKIPSSL=false

# ============================================================================
# GITLAB CONFIGURATION (Optional)
# ============================================================================

# GitLab OAuth credentials
#MODERNE_AGENT_GITLAB_0_OAUTH_CLIENTID=<gitlab-oauth-client-id>
#MODERNE_AGENT_GITLAB_0_OAUTH_CLIENTSECRET=<gitlab-oauth-client-secret>

# GitLab instance URL
#MODERNE_AGENT_GITLAB_0_URL=https://gitlab.com

# Groups allowed to use this agent
#MODERNE_AGENT_GITLAB_0_ALLOWABLE_GROUPS_0=my-group
#MODERNE_AGENT_GITLAB_0_ALLOWABLE_GROUPS_1=another-group

# Skip SSL verification
#MODERNE_AGENT_GITLAB_0_SKIPSSL=false

# ============================================================================
# BITBUCKET DATA CENTER CONFIGURATION (Optional)
# ============================================================================

# Bitbucket Data Center OAuth credentials
#MODERNE_AGENT_BITBUCKET_0_OAUTH_CLIENTID=<bitbucket-oauth-client-id>
#MODERNE_AGENT_BITBUCKET_0_OAUTH_CLIENTSECRET=<bitbucket-oauth-client-secret>

# Bitbucket instance URL
#MODERNE_AGENT_BITBUCKET_0_URL=https://bitbucket.mycompany.com

# Projects allowed to use this agent
#MODERNE_AGENT_BITBUCKET_0_ALLOWABLE_PROJECTS_0=PROJ1
#MODERNE_AGENT_BITBUCKET_0_ALLOWABLE_PROJECTS_1=PROJ2

# Skip SSL verification
#MODERNE_AGENT_BITBUCKET_0_SKIPSSL=false

# ============================================================================
# BITBUCKET CLOUD CONFIGURATION (Optional)
# ============================================================================

# Bitbucket Cloud OAuth credentials
#MODERNE_AGENT_BITBUCKETCLOUD_0_OAUTH_CLIENTID=<bitbucket-cloud-oauth-client-id>
#MODERNE_AGENT_BITBUCKETCLOUD_0_OAUTH_CLIENTSECRET=<bitbucket-cloud-oauth-client-secret>

# Workspaces allowed to use this agent
#MODERNE_AGENT_BITBUCKETCLOUD_0_ALLOWABLE_WORKSPACES_0=my-workspace

# ============================================================================
# AZURE DEVOPS CONFIGURATION (Optional)
# ============================================================================

# Azure DevOps OAuth credentials
#MODERNE_AGENT_AZURE_0_OAUTH_CLIENTID=<azure-oauth-client-id>
#MODERNE_AGENT_AZURE_0_OAUTH_CLIENTSECRET=<azure-oauth-client-secret>

# Azure DevOps organization URL
#MODERNE_AGENT_AZURE_0_URL=https://dev.azure.com/myorganization

# Organizations allowed to use this agent
#MODERNE_AGENT_AZURE_0_ALLOWABLE_ORGANIZATIONS_0=my-org

# ============================================================================
# ARTIFACTORY CONFIGURATION (Optional - Recommended for LST storage)
# ============================================================================

# Artifactory instance URL
#MODERNE_AGENT_ARTIFACTORY_0_URL=https://artifactory.mycompany.com/artifactory

# Artifactory authentication (username/password or bearer token)
#MODERNE_AGENT_ARTIFACTORY_0_USERNAME=<artifactory-username>
#MODERNE_AGENT_ARTIFACTORY_0_PASSWORD=<artifactory-password>
# OR use bearer token:
#MODERNE_AGENT_ARTIFACTORY_0_BEARERTOKEN=<artifactory-token>

# Artifactory repositories to search for LSTs
#MODERNE_AGENT_ARTIFACTORY_0_LST_0_REPO=libs-release-local
#MODERNE_AGENT_ARTIFACTORY_0_LST_1_REPO=libs-snapshot-local

# Skip SSL verification
#MODERNE_AGENT_ARTIFACTORY_0_SKIPSSL=false

# AQL query filters (advanced - see documentation)
#MODERNE_AGENT_ARTIFACTORY_0_ASTQUERYFILTERS_0=@name ~ "*.jar"

# ============================================================================
# MAVEN REPOSITORY CONFIGURATION (Optional - Alternative to Artifactory)
# ============================================================================

# Maven repository URL
#MODERNE_AGENT_MAVEN_0_URL=https://repo.maven.apache.org/maven2

# Maven repository authentication (if required)
#MODERNE_AGENT_MAVEN_0_USERNAME=<maven-username>
#MODERNE_AGENT_MAVEN_0_PASSWORD=<maven-password>

# Local path for Maven repository (for locally built LSTs)
#MODERNE_AGENT_MAVEN_0_LOCALREPOSITORY=/home/user/.m2/repository

# Skip SSL verification
#MODERNE_AGENT_MAVEN_0_SKIPSSL=false

# Maven repository for POM resolution (typically Maven Central)
#MODERNE_AGENT_MAVEN_0_RELEASES_URL=https://repo.maven.apache.org/maven2

# ============================================================================
# PACKAGE MANAGEMENT (Optional - for Python, etc.)
# ============================================================================

# Python pip configuration
#MODERNE_AGENT_PIP_0_URL=https://pypi.org/simple

# ============================================================================
# LOGGING (Optional)
# ============================================================================

# Enable debug logging
#LOGGING_LEVEL_IO_MODERNE_AGENT=DEBUG

# ============================================================================
# JVM TUNING (Set via docker run or Kubernetes)
# ============================================================================

# These are typically set as Java system properties, not environment variables:
# -XX:MaxRAMPercentage=65.0
# -XX:MaxDirectMemorySize=2G
# -XX:+HeapDumpOnOutOfMemoryError
# -XX:+UseStringDeduplication
