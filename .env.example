# ============================================================================
# CORE AGENT CONFIGURATION (Required)
# ============================================================================

# URI for connecting to the Moderne API Gateway (provided by Moderne)
# Format: https://api.<tenant>.moderne.io/rsocket
MODERNE_AGENT_APIGATEWAYRSOCKETURI=https://api.<tenant>.moderne.io/rsocket

# 256-bit AES encryption key in hex format for artifact encryption
# Generate with: openssl enc -aes-256-cbc -k secret -P -md sha256
# Use the value after "key="
MODERNE_AGENT_CRYPTO_SYMMETRICKEY=<your-256-bit-hex-key>

# Identifier for this agent in the Moderne dashboard
# This will be visible in metrics and the UI (use lowercase with hyphens)
MODERNE_AGENT_NICKNAME=my-agent

# Agent connection token (provided by Moderne)
MODERNE_AGENT_TOKEN=<your-agent-token>

# ============================================================================
# ARTIFACTORY CONFIGURATION (Optional - Recommended for LST storage)
# ============================================================================

# Artifactory instance URL. Should end with /artifactory
#MODERNE_AGENT_ARTIFACTORY_0_URL=https://artifactory.mycompany.com/artifactory

# Artifactory authentication - use EITHER username/password OR bearerToken
#MODERNE_AGENT_ARTIFACTORY_0_USERNAME=<artifactory-username>
#MODERNE_AGENT_ARTIFACTORY_0_PASSWORD=<artifactory-password>
#MODERNE_AGENT_ARTIFACTORY_0_BEARERTOKEN=<artifactory-token>

# Skip SSL verification
#MODERNE_AGENT_ARTIFACTORY_0_SKIPSSL=false

# AQL query filters (required when using Artifactory)
# IMPORTANT: Use single quotes around the value to prevent $match from being interpreted as a variable
#MODERNE_AGENT_ARTIFACTORY_0_ASTQUERYFILTERS_0='"repo":{"$match":"moderne-lsts"}'
#MODERNE_AGENT_ARTIFACTORY_0_ASTQUERYFILTERS_1='"name":{"$match":"*-ast.jar"}'

# ============================================================================
# MAVEN REPOSITORY CONFIGURATION (Optional - Alternative to Artifactory, and used for dependency resolution)
# ============================================================================

# Maven repository URL
#MODERNE_AGENT_MAVEN_0_URL=https://artifactory.mycompany.com/artifactory/my-repository

# Maven repository authentication (if required)
#MODERNE_AGENT_MAVEN_0_USERNAME=<maven-username>
#MODERNE_AGENT_MAVEN_0_PASSWORD=<maven-password>

# Local path for Maven repository (cache dir, must be unique)
#MODERNE_AGENT_MAVEN_0_LOCALREPOSITORY=~/.my-repository

# Skip SSL verification
#MODERNE_AGENT_MAVEN_0_SKIPSSL=false

# Enable release artifacts (default: true)
#MODERNE_AGENT_MAVEN_0_RELEASES=true

# Enable snapshot artifacts (default: true)
#MODERNE_AGENT_MAVEN_0_SNAPSHOTS=true

# Use as LST source (default: true)
#MODERNE_AGENT_MAVEN_0_ASTSOURCE=true

# Use as recipe source (default: true)
#MODERNE_AGENT_MAVEN_0_RECIPESOURCE=true

# ============================================================================
# GITHUB CONFIGURATION (Optional)
# ============================================================================

# GitHub OAuth credentials for authentication
#MODERNE_AGENT_GITHUB_0_OAUTH_CLIENTID=<github-oauth-client-id>
#MODERNE_AGENT_GITHUB_0_OAUTH_CLIENTSECRET=<github-oauth-client-secret>

# GitHub instance URL (use https://github.com for GitHub.com)
#MODERNE_AGENT_GITHUB_0_URL=https://github.com

# Organizations allowed to use this agent (supports wildcards)
#MODERNE_AGENT_GITHUB_0_ALLOWABLE_ORGANIZATIONS_0=my-org
#MODERNE_AGENT_GITHUB_0_ALLOWABLE_ORGANIZATIONS_1=another-org

# Include private repositories
#MODERNE_AGENT_GITHUB_0_OAUTH_INCLUDEPRIVATEREPOS=true

# Alternative URLs for this GitHub instance (defaults to ssh url it set up below)
#MODERNE_AGENT_GITHUB_0_ALTERNATEURLS_0=

# Skip SSL verification (not recommended for production)
#MODERNE_AGENT_GITHUB_0_SKIPSSL=false

# SSH configuration (optional)
#MODERNE_AGENT_GITHUB_0_SSH_PRIVATEKEY=
#MODERNE_AGENT_GITHUB_0_SSH_PASSPHRASE=
#MODERNE_AGENT_GITHUB_0_SSH_SSHFILENAME=
#MODERNE_AGENT_GITHUB_0_SSH_USER=git
#MODERNE_AGENT_GITHUB_0_SSH_PORT=22

# ============================================================================
# GITLAB CONFIGURATION (Optional)
# ============================================================================

# GitLab OAuth credentials
#MODERNE_AGENT_GITLAB_0_OAUTH_CLIENTID=<gitlab-oauth-client-id>
#MODERNE_AGENT_GITLAB_0_OAUTH_CLIENTSECRET=<gitlab-oauth-client-secret>

# GitLab instance URL
#MODERNE_AGENT_GITLAB_0_URL=https://gitlab.com

# Alternative URLs for this GitLab instance (defaults to ssh url it set up below)
#MODERNE_AGENT_GITLAB_0_ALTERNATEURLS_0=

# Skip SSL verification
#MODERNE_AGENT_GITLAB_0_SKIPSSL=false

# SSH configuration (optional)
#MODERNE_AGENT_GITLAB_0_SSH_PRIVATEKEY=
#MODERNE_AGENT_GITLAB_0_SSH_PASSPHRASE=
#MODERNE_AGENT_GITLAB_0_SSH_SSHFILENAME=
#MODERNE_AGENT_GITLAB_0_SSH_USER=git
#MODERNE_AGENT_GITLAB_0_SSH_PORT=22

# ============================================================================
# BITBUCKET DATA CENTER CONFIGURATION (Optional)
# ============================================================================

# Bitbucket instance URL
#MODERNE_AGENT_BITBUCKET_0_URL=https://bitbucket.mycompany.com

# Bitbucket Data Center OAuth credentials
#MODERNE_AGENT_BITBUCKET_0_OAUTH_KEY=<bitbucket-oauth-key>
#MODERNE_AGENT_BITBUCKET_0_OAUTH_SECRET=<bitbucket-oauth-secret>

# Bitbucket Data Center private key (deprecated: for application links)
#MODERNE_AGENT_BITBUCKET_0_PRIVATEKEY=

# Alternative URLs for this Bitbucket instance (defaults to ssh url it set up below)
#MODERNE_AGENT_BITBUCKET_0_ALTERNATEURLS_0=

# Skip SSL verification
#MODERNE_AGENT_BITBUCKET_0_SKIPSSL=false

# SSH configuration (optional)
#MODERNE_AGENT_BITBUCKET_0_SSH_PRIVATEKEY=
#MODERNE_AGENT_BITBUCKET_0_SSH_PASSPHRASE=
#MODERNE_AGENT_BITBUCKET_0_SSH_SSHFILENAME=
#MODERNE_AGENT_BITBUCKET_0_SSH_USER=git
#MODERNE_AGENT_BITBUCKET_0_SSH_PORT=7999

# ============================================================================
# BITBUCKET CLOUD CONFIGURATION (Optional)
# ============================================================================

# Bitbucket Cloud OAuth credentials (note: no index, single instance only)
#MODERNE_AGENT_BITBUCKET_CLOUD_OAUTH_KEY=<bitbucket-cloud-oauth-key>
#MODERNE_AGENT_BITBUCKET_CLOUD_OAUTH_SECRET=<bitbucket-cloud-oauth-secret>

# ============================================================================
# AZURE DEVOPS CONFIGURATION (Optional)
# ============================================================================

# Azure DevOps OAuth credentials
#MODERNE_AGENT_AZUREDEVOPS_0_OAUTH_CLIENTID=<azure-oauth-client-id>
#MODERNE_AGENT_AZUREDEVOPS_0_OAUTH_CLIENTSECRET=<azure-oauth-client-secret>
#MODERNE_AGENT_AZUREDEVOPS_0_OAUTH_TENANTID=<azure-tenant-id>

# Skip SSL verification
#MODERNE_AGENT_AZUREDEVOPS_0_SKIPSSL=false

# SSH configuration (optional)
#MODERNE_AGENT_AZUREDEVOPS_0_SSH_PRIVATEKEY=
#MODERNE_AGENT_AZUREDEVOPS_0_SSH_PASSPHRASE=
#MODERNE_AGENT_AZUREDEVOPS_0_SSH_SSHFILENAME=
#MODERNE_AGENT_AZUREDEVOPS_0_SSH_USER=git

# ============================================================================
# PERFORMANCE TUNING (Optional)
# ============================================================================

# Number of parallel threads for downloading LSTs (default: 2)
# Increase for better throughput if network and CPU allow
#MODERNE_AGENT_DOWNLOADPARALLELISM=4

# Frequency of LST artifact index updates in seconds (default: 120)
# Lower values provide fresher data but increase load
#MODERNE_AGENT_ARTIFACTINDEXINTERVALSECONDS=120

# ============================================================================
# ORGANIZATION HIERARCHY (Optional)
# ============================================================================

# Load organization structure from HTTP(S) endpoint (repos.csv format)
#MODERNE_AGENT_ORGANIZATION_REPOSCSV=https://example.com/repos.csv

# Organization data update interval in seconds
#MODERNE_AGENT_ORGANIZATION_UPDATEINTERVALSECONDS=

# External organization service URL
#MODERNE_AGENT_ORGANIZATION_SERVICE_URL=

# Skip SSL for organization service
#MODERNE_AGENT_ORGANIZATION_SERVICE_SKIPSSL=false

# ============================================================================
# COMMIT OPTIONS (Optional)
# ============================================================================

# Restrict available commit strategies
# Options: Direct, Branch, Fork, PullRequest, ForkAndPullRequest
#MODERNE_AGENT_DEFAULTCOMMITOPTIONS_0=PullRequest
#MODERNE_AGENT_DEFAULTCOMMITOPTIONS_1=ForkAndPullRequest

# ============================================================================
# PYPI CONFIGURATION (Optional - for Python packages)
# ============================================================================

# PyPI repository URL
#MODERNE_AGENT_PYPI_0_URL=https://pypi.org/simple

# PyPI authentication (if required)
#MODERNE_AGENT_PYPI_0_USERNAME=<pypi-username>
#MODERNE_AGENT_PYPI_0_PASSWORD=<pypi-password>

# Skip SSL verification
#MODERNE_AGENT_PYPI_0_SKIPSSL=false

# ============================================================================
# RECIPE CONFIGURATION (Optional)
# ============================================================================

# Only use explicitly configured recipe sources
#MODERNE_AGENT_RECIPE_USEONLYCONFIGURED=false

# Only use explicitly configured visualization sources
#MODERNE_AGENT_VISUALIZATION_USEONLYCONFIGURED=false

# POM cache type (inmemory or redis)
#MODERNE_AGENT_RECIPE_POMCACHE_TYPE=inmemory

# POM cache entry TTL in minutes
#MODERNE_AGENT_RECIPE_POMCACHE_ENTRYTTLMINUTES=60

# Redis cache configuration (if type=redis)
#MODERNE_AGENT_RECIPE_POMCACHE_REDIS_HOST=localhost
#MODERNE_AGENT_RECIPE_POMCACHE_REDIS_PORT=6379
#MODERNE_AGENT_RECIPE_POMCACHE_REDIS_USERNAME=
#MODERNE_AGENT_RECIPE_POMCACHE_REDIS_PASSWORD=
#MODERNE_AGENT_RECIPE_POMCACHE_REDIS_SSL=false
#MODERNE_AGENT_RECIPE_POMCACHE_REDIS_DATABASE=0

# ============================================================================
# HTTP PROXY CONFIGURATION (Optional)
# ============================================================================

# Proxy for API gateway connections
#MODERNE_AGENT_APIGATEWAY_PROXY_HOST=
#MODERNE_AGENT_APIGATEWAY_PROXY_PORT=

# ============================================================================
# GENERIC HTTP TOOLS (Optional)
# ============================================================================

# HTTP tool configuration for external integrations
#MODERNE_AGENT_HTTPTOOL_0_URL=
#MODERNE_AGENT_HTTPTOOL_0_USERNAME=
#MODERNE_AGENT_HTTPTOOL_0_PASSWORD=
#MODERNE_AGENT_HTTPTOOL_0_BEARERTOKEN=
#MODERNE_AGENT_HTTPTOOL_0_SKIPSSL=false

# ============================================================================
# UI CUSTOMIZATION (Optional)
# ============================================================================

# Custom help links in the UI
#MODERNE_AGENT_UI_MOREHELP_0_LABEL=Internal Wiki
#MODERNE_AGENT_UI_MOREHELP_0_URL=https://wiki.mycompany.com/moderne

# CLI download instructions
#MODERNE_AGENT_CLI_DOWNLOADINSTRUCTIONS_LABEL=Download CLI
#MODERNE_AGENT_CLI_DOWNLOADINSTRUCTIONS_URL=https://wiki.mycompany.com/moderne-cli

# ============================================================================
# MULTI-TENANT CONFIGURATION (Optional)
# ============================================================================

# Tenant domain for private recipe support and other multi-tenant related features
#MODERNE_AGENT_TENANTDOMAIN=

# ============================================================================
# LOGGING (Optional)
# ============================================================================

# Enable debug logging
#LOGGING_LEVEL_IO_MODERNE_AGENT=DEBUG
